    <property name="countryReturnShortName" display-name-key="Country Return Short Name" description-key="Return country as code instead of full name" of-type="TwoOptions" usage="input" required="false" />
    <property name="street" display-name-key="Street" description-key="Street number and name from selected address" of-type="SingleLine.Text" usage="bound" required="true" />
    <property name="suburb" display-name-key="Suburb" description-key="Suburb/locality from selected address" of-type="SingleLine.Text" usage="bound" required="false" />
